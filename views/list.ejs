<%- include("header"); -%>
    <div class="box" id="heading">
        <h1>
            <%= listTitle%>
        </h1>
    </div>
    <div class="box">

       <% newListItem.forEach(function(item){ %>
            <div class="item">
                <input type="checkbox">
                <p><%=item.name%></p>
            </div>
       <% }) %>


        <% for(var i=0; i <newListItem.length;i++){ %>
            <div class="item">
            <input type="checkbox">
            <p>
                <%= newListItem[i].name %>
            </p>
    </div>

    <% } %>



        <form class="" action="/" method="post">
            <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
            <button type="submit" name="list" value=<%=listTitle %>>+</button>
            <!-- <button type="submit" name="deleteButton" class="deletebtn">Delete</button> -->
        </form>

        </div>

        <%- include("footer"); -%>
